package StringsPrograms;

public class RemoveWhiteSpaces {
	
	public static void main(String[] args) {

		    String S="geek for geeks";
		    char[] ch=S.toCharArray();
		
		    String str=""; 
	        for(int i=0;i<ch.length;i++){
	           
	           if(ch[i] !=' ');
	           {
	               str=str+S.charAt(i);
	           }
	        } 
	        System.out.println(str);
	    }

}
