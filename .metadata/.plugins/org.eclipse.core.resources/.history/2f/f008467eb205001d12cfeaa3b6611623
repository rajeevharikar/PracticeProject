package StringsPrograms;

public class Combine2StringsAndReverse {
	
	public static void main(String[] args) {
		String S1="Java";
		String S2="Selenium";
		
		 S1= S1 + S2;
	       
	       String rev="";
	       for(int i=S1.length()-1;i>=0;i--){
		           
		           rev=rev+S1.charAt(i);
		       }
	       System.out.println(rev);
	}

}
